@model Starter.Models.BandModel
<partial name="_Band"/> 
        
<button class="btn btn-primary">
    <a class="nav-link text-dark" asp-area="" asp-controller="Primary" asp-action="CreateAlbumForm" asp-route-bandID = "@Model.id">Add Album</a>
</button>

<div class="card-group">
  <div class="card">
    @foreach (AlbumModel album in @Model.albums)
    {
        <div class="card-body">
        <h5 class="card-title">@album.title</h5>
        </div>
    }
  </div>
</div>
